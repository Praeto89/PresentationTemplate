    <!-- Subtopic Detail Panel CSS -->
    <style>
        .subtopic-detail-panel {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 1000;
            animation: fadeIn 0.2s ease-in-out;
        }
        
        .subtopic-detail-panel.active {
            display: flex;
            align-items: center;
            justify-content: center;
        }
        
        .subtopic-detail-container {
            background-color: white;
            border-radius: 8px;
            max-width: 700px;
            width: 90%;
            max-height: 80vh;
            overflow-y: auto;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);
            animation: slideUp 0.3s ease-out;
        }
        
        .subtopic-detail-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 30px;
            border-bottom: 2px solid #f0f0f0;
        }
        
        .subtopic-detail-title {
            margin: 0;
            font-size: 28px;
            color: #333;
            font-weight: 600;
        }
        
        .subtopic-detail-close {
            background: none;
            border: none;
            font-size: 32px;
            color: #999;
            cursor: pointer;
            padding: 0;
            width: 40px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 50%;
            transition: background-color 0.2s, color 0.2s;
        }
        
        .subtopic-detail-close:hover {
            background-color: #f0f0f0;
            color: #333;
        }
        
        .subtopic-detail-content {
            padding: 30px;
            font-size: 16px;
            line-height: 1.6;
            color: #555;
        }
        
        .subtopic-detail-content p {
            margin: 0 0 16px 0;
        }
        
        .subtopic-detail-content p:last-child {
            margin-bottom: 0;
        }
        
        @keyframes fadeIn {
            from {
                opacity: 0;
            }
            to {
                opacity: 1;
            }
        }
        
        @keyframes slideUp {
            from {
                opacity: 0;
                transform: translateY(20px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }
    </style>
</head>
